{
  "manifest_version": 3,
  "name": "Bookmarks mind",
  "description": "Converts bookmarks into mind maps",
  "version": "1.0",
  "action": {
    "default_title": "Click to view chrome bookmarks mindmap",
    "default_icon": {
      "16": "extension_icon@16px.png",
      "32": "extension_icon@32px.png",
      "48": "extension_icon@48px.png",
      "128": "extension_icon@128px.png"
    }
  },
  "icons": {
    "16": "extension_icon@16px.png",
    "32": "extension_icon@32px.png",
    "48": "extension_icon@48px.png",
    "128": "extension_icon@128px.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["bookmarks", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
